<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Organizator_Proslava.ViewModel"
                    xmlns:v="clr-namespace:Organizator_Proslava.View"
                    xmlns:cfvm="clr-namespace:Organizator_Proslava.ViewModel.CollaboratorForm"
                    xmlns:cfv="clr-namespace:Organizator_Proslava.View.CollaboratorForm"

                    xmlns:alert="clr-namespace:Organizator_Proslava.Dialogs.Alert"
                    xmlns:option="clr-namespace:Organizator_Proslava.Dialogs.Option"
                    xmlns:map="clr-namespace:Organizator_Proslava.Dialogs.Map">

    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <v:LoginView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegisterViewModel}">
        <v:RegisterView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SpaceViewModel}">
        <v:Space />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ImageSelectViewModel}">
        <v:ImageSelectView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cfvm:CollaboratorFormViewModel}">
        <cfv:CollaboratorFormView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cfvm:SelectCollaboratorTypeViewModal}">
        <cfv:SelectCollaboratorTypeView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cfvm:IndividualCollaboratorInformationsViewModel}">
        <cfv:IndividualCollaboratorInformationsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cfvm:LegalCollaboratorInformationsViewModel}">
        <cfv:LegalCollaboratorInformationsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type alert:AlertDialogViewModel}">
        <alert:AlertDialogView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type option:OptionDialogViewModel}">
        <option:OptionDialogView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type map:MapDialogViewModel}">
        <map:MapDialogView />
    </DataTemplate>
</ResourceDictionary>