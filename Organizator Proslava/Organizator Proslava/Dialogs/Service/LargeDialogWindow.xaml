<Window x:Class="Organizator_Proslava.Dialogs.Service.LargeDialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:v="clr-namespace:Organizator_Proslava.View"
        xmlns:local="clr-namespace:Organizator_Proslava.Dialogs.Service"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="560" Width="560"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <v:Background Opacity="0.3" Margin="-170,0,-378,-341" />
        <ContentControl x:Name="ContentPresenter" Content="{Binding}" />
    </Grid>
</Window>